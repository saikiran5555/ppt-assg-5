    "def countCompleteRows(n):\n",
    "    row = 1  \n",
    "    while n >= row:\n",
    "        n -= row \n",
    "        row += 1  \n",
    "    return row - 1  \n",
    "n = 5\n",
    "print(countCompleteRows(n))\n",
    "\n"
   ]
